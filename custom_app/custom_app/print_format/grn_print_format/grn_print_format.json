 "font_size": 14,
 "html": "<style>\r\n    * {\r\n        box-sizing: border-box;\r\n        font-family: Arial, sans-serif;\r\n        font-size: 11px;\r\n    }\r\n\r\n    body { margin: 0; padding: 0; }\r\n\r\n    .container {\r\n        width: 100%;\r\n        max-width: 210mm;\r\n        margin: auto;\r\n        padding: 8mm;\r\n    }\r\n\r\n    table {\r\n        width: 100%;\r\n        border-collapse: collapse;\r\n        table-layout: fixed;\r\n        word-wrap: break-word;\r\n    }\r\n\r\n    .border td, .border th {\r\n        border: 1px solid #000;\r\n        padding: 4px 6px;\r\n        vertical-align: top;\r\n        white-space: normal;\r\n    }\r\n\r\n    .center { text-align: center; }\r\n    .right { text-align: right; }\r\n    .bold { font-weight: bold; }\r\n    .title { font-size: 18px; font-weight: bold; }\r\n\r\n    img { max-width: 100%; height: auto; }\r\n</style>\r\n\r\n<div class=\"container\">\r\n\r\n    <!-- HEADER -->\r\n    <table style=\"margin-bottom:6px;\">\r\n        <tr>\r\n            <td width=\"18%\">\r\n                <img src=\"/files/IFIM.png\" style=\"height:75px;\">\r\n            </td>\r\n\r\n            <td width=\"64%\" class=\"center\">\r\n                <div class=\"title\">IFIM INSTITUTIONS</div>\r\n                <div>8P &amp; 9P, KIADB Industrial Area, Electronics City Phase I</div>\r\n                <div>Bangalore - 560100 | Tel: +91 80 41432800</div>\r\n                <div>E-mail: info@ifim.edu.in | Website: www.ifim.edu.in</div>\r\n            </td>\r\n\r\n            <td width=\"18%\"></td>\r\n        </tr>\r\n    </table>\r\n\r\n    <div class=\"center bold\" style=\"margin-bottom:4px;\">:grn detail:</div>\r\n\r\n    <!-- GRN DETAILS -->\r\n    <!-- GRN DETAILS -->\r\n<table class=\"border\">\r\n\r\n    <tr>\r\n        <td colspan=\"6\">\r\n            <b>GRN No :</b><br>\r\n            {{ doc.name }}\r\n        </td>\r\n    </tr>\r\n\r\n    <tr>\r\n        <td colspan=\"2\">\r\n            <b>grn created for :</b><br>\r\n            {{ doc.purchase_order or \"\" }}\r\n        </td>\r\n\r\n        <td colspan=\"2\">\r\n            <b>Received On :</b><br>\r\n            {{ doc.posting_date }}\r\n        </td>\r\n\r\n        <td colspan=\"2\">\r\n            <b>Store Name :</b><br>\r\n            {{ doc.set_warehouse or \"\" }}\r\n        </td>\r\n    </tr>\r\n\r\n    <!-- FIXED ROW (was colspan 1.5 + 1.5) -->\r\n    <tr>\r\n        <td colspan=\"3\">\r\n            <b>Vendor Name :</b><br>\r\n            {{ doc.supplier }}\r\n        </td>\r\n\r\n        <td colspan=\"3\">\r\n            <b>Vendor Address :</b><br>\r\n            {{ doc.supplier_address or \"\" }}\r\n        </td>\r\n    </tr>\r\n\r\n    <tr>\r\n        <td colspan=\"2\">\r\n            <b>Store Entry Date :</b><br>\r\n            {{ doc.posting_date }}\r\n        </td>\r\n\r\n        <td colspan=\"2\">\r\n            <b>Delivery Challan No :</b><br>\r\n            {{ doc.supplier_delivery_note or \"\" }}\r\n        </td>\r\n\r\n        <td colspan=\"2\">\r\n            <b>DC Date :</b><br>\r\n            {{ doc.posting_date }}\r\n        </td>\r\n    </tr>\r\n\r\n    <tr>\r\n        <td colspan=\"2\">\r\n            <b>Mode of Transport :</b><br>\r\n            {{ doc.mode_of_transport or \"\" }}\r\n        </td>\r\n\r\n        <td colspan=\"2\">\r\n            <b>grn employee :</b><br>\r\n            {{ doc.owner }}\r\n        </td>\r\n<td colspan=2>\r\n        <b>Verifying Employee :</b><br>\r\n{% if doc.docstatus == 1 %}\r\n    {{ frappe.db.get_value(\"User\", doc.modified_by, \"full_name\") }}\r\n{% else %}\r\n    Pending\r\n{% endif %}\r\n</td>\r\n    </tr>\r\n\r\n    <tr>\r\n        <td colspan=\"2\">\r\n            <b>Verifying Emp Dept :</b><br>\r\n{% if doc.docstatus == 1 %}\r\n    {% set emp = frappe.db.get_value(\"Employee\",\r\n        {\"user_id\": doc.modified_by},\r\n        \"department\") %}\r\n    {{ emp or \"\" }}\r\n{% endif %}\r\n        </td>\r\n\r\n        <td colspan=\"2\">\r\n            <b>Verification Status :</b><br>\r\n            {% if doc.docstatus == 1 %}\r\n    Verified\r\n{% else %}\r\n    Pending\r\n{% endif %}\r\n        </td>\r\n\r\n        <td colspan=\"2\">\r\n            <b>Voucher Status :</b><br>\r\n            {{ doc.status }}\r\n        </td>\r\n    </tr>\r\n\r\n    <!--<tr>-->\r\n    <!--    <td colspan=\"2\">-->\r\n    <!--        <b>Invoice No :</b><br>-->\r\n    <!--        {{ doc.supplier_delivery_note or \"\" }}-->\r\n    <!--    </td>-->\r\n\r\n    <!--    <td colspan=\"2\">-->\r\n    <!--        <b>Invoice Date :</b><br>-->\r\n    <!--        {{ frappe.utils.formatdate(doc.bill_date) if doc.bill_date else \"\" }}-->\r\n    <!--    </td>-->\r\n\r\n    <!--    <td colspan=\"2\">-->\r\n    <!--        Remarks:-->\r\n    <!--    </td>-->\r\n    <!--</tr>-->\r\n\r\n    <!-- FIXED HALF WIDTH ROWS -->\r\n    <!--<tr>-->\r\n    <!--    <td colspan=\"3\">-->\r\n    <!--        <b>Installation :</b><br>-->\r\n    <!--    </td>-->\r\n\r\n    <!--    <td colspan=\"3\">-->\r\n    <!--        <b>Warranty :</b><br>-->\r\n    <!--    </td>-->\r\n    <!--</tr>-->\r\n\r\n    <!--<tr>-->\r\n    <!--    <td colspan=\"3\">-->\r\n    <!--        <b>Performance Bank Guarantee(PBG) :</b><br>-->\r\n    <!--    </td>-->\r\n\r\n    <!--    <td colspan=\"3\">-->\r\n    <!--        <b>Any Other :</b><br>-->\r\n    <!--    </td>-->\r\n    <!--</tr>-->\r\n\r\n</table>\r\n\r\n    <br>\r\n\r\n    <!-- ITEM TABLE -->\r\n\r\n    <table class=\"border\">\r\n        <thead>\r\n            <tr class=\"center bold\">\r\n               <th colspan=\"9\" class=\"center\">grn history :</th> \r\n            </tr>\r\n            <tr class=\"center bold\">\r\n                <th width=\"5%\">Sl No</th>\r\n                <th width=\"15%\">Item Code</th>\r\n                <th width=\"25%\">Item Name</th>\r\n                <th width=\"10%\">Unit Price (INR)</th>\r\n                <th width=\"10%\">Received Qty</th>\r\n                <th width=\"10%\">Damage Qty</th>\r\n                <th width=\"8%\">Disc.</th>\r\n                <th width=\"7%\">Taxes</th>\r\n                <th width=\"10%\">Net Total (INR)</th>\r\n            </tr>\r\n        </thead>\r\n        <tbody>\r\n            {% for row in doc.items %}\r\n            <tr>\r\n                <td class=\"center\">{{ loop.index }}</td>\r\n                <td>{{ row.item_code }}</td>\r\n                <td>{{ row.item_name }}</td>\r\n                <td class=\"right\">{{ \"%.2f\"|format(row.rate) }}</td>\r\n                <td class=\"center\">{{ row.qty }}</td>\r\n                <td class=\"center\">0</td>\r\n                <td class=\"right\">0.00</td>\r\n                <td class=\"right\">0.00</td>\r\n                <td class=\"right\">{{ \"%.2f\"|format(row.amount) }}</td>\r\n            </tr>\r\n            {% endfor %}\r\n            <!--<tr>-->\r\n            <!--    <td class=\"right bold\" colspan=\"8\">Discount (INR)</td>-->\r\n            <!--    <td width=\"20%\" class=\"right\">{{ \"%.2f\"|format(doc.total_taxes_and_charges or 0) }}</td>-->\r\n            <!--</tr>-->\r\n            <!--<tr>-->\r\n            <!--    <td class=\"right bold\" colspan=\"8\">Taxes (INR)</td>-->\r\n            <!--    <td width=\"20%\" class=\"right\">{{ \"%.2f\"|format(doc.total_taxes_and_charges or 0) }}</td>-->\r\n            <!--</tr>-->\r\n            <!--<tr>-->\r\n            <!--    <td class=\"right bold\" colspan=\"8\">Other Charges + Mise Charges + PF Charges (INR)</td>-->\r\n            <!--    <td class=\"right\">0.00</td>-->\r\n            <!--</tr>-->\r\n            <tr>\r\n                <td class=\"right bold\" colspan=\"8\">Total Net Amount (INR)</td>\r\n                <td class=\"right bold\">{{ \"%.2f\"|format(doc.grand_total) }}</td>\r\n            </tr>\r\n            <tr class=\"bold\">\r\n                <td colspan=\"9\">Amount Chargeable (in words): {{ doc.in_words }}</td>\r\n            </tr>\r\n        </tbody>\r\n    </table>\r\n    \r\n\r\n</div>\r\n",
